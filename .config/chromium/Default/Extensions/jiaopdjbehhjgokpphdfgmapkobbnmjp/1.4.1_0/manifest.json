{
   "action": {
      "default_icon": {
         "128": "icons/icon128.png",
         "32": "icons/icon32.png",
         "64": "icons/icon64.png"
      },
      "default_popup": "popup.html",
      "default_title": "Youtube-shorts block"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "main.css" ],
      "js": [ "main.js" ],
      "matches": [ "*://*.youtube.com/*", "*://m.youtube.com/*" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_ext_desc__",
   "icons": {
      "128": "icons/icon128.png",
      "32": "icons/icon32.png",
      "64": "icons/icon64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Youtube-shorts block",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.1",
   "web_accessible_resources": [ {
      "matches": [ "*://*.youtube.com/*" ],
      "resources": [ "icons/to_normal.svg" ]
   } ]
}
